<!-- Start Footer Area -->
<div class="rn-footer-one rn-section-gap bg-color--1 mt--100 mt_md--80 mt_sm--80">
    <div class="container">
        <div class="row gx-5">
            <div class="col-lg-5 col-md-6 col-sm-6 col-12">
                <div class="widget-content-wrapper">
                    <div class="footer-left">
                        <div class="logo-thumbnail logo-custom-css">
                            <a class="logo-light" href="/"><img src="<%= settings.siteLogo%>"
                                    alt="logo"  style="padding-top: 5px; max-height: 70px !important; max-width: 140px;"></a>
                            <a class="logo-dark" href="/"><img src="<%= settings.siteLogo%>"
                                    alt="logo" style="padding-top: 5px; max-height: 70px !important; max-width: 140px;"></a>
                        </div>
                        <p class="rn-footer-describe">
                            <%= settings.footerDesc%>
                        </p>
                    </div>
                    <div class="widget-bottom mt--40 pt--40">
                        <h6 class="title">En Son Güncellemeleri Alın </h6>
                        <div class="input-group">
                            <input type="text" class="form-control bg-color--2" placeholder="Mail adresiniz"
                                aria-label="Recipient's username">
                            <div class="input-group-append">
                                <button class="btn btn-primary-alta btn-outline-secondary"
                                    type="button">Takip Et</button>
                            </div>
                        </div>
                        <div class="newsletter-dsc">
                            <p>Email is safe. We don't spam.</p>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-lg-4 col-md-6 col-sm-6 col-12 mt_mobile--40">
                <div class="footer-widget widget-quicklink">
                    <h6 class="widget-title">Birkaç Ürünümüz</h6>
                    <ul class="footer-list-one">
                        <% allProduct.slice(0, 6).forEach((product) => {%>
                        <li class="single-list"><a href="/urunler/<%= product.productLink%>"><%= product.productName%></a></li>
                        <% }) %>
                    </ul>
                </div>
            </div>

            <div class="col-lg-3 col-md-6 col-sm-6 col-12 mt_md--40 mt_sm--40">
                <div class="footer-widget widget-information">
                    <h6 class="widget-title">Sayfalar</h6>
                    <ul class="footer-list-one">
                        <% settings.footerMenu.forEach((setMenu) => {%>
                        <li class="single-list"><a href="<%= setMenu.menuLink%>"><%= setMenu.menuName%></a></li>
                        <% }) %>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Footer Area -->
<!-- Start Footer Area -->
<div class="copy-right-one ptb--20 bg-color--1">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 col-md-12 col-sm-12">
                <div class="copyright-left">
                    <span><%= settings.footerCopyright%></span>
                </div>
            </div>
            <div class="col-lg-6 col-md-12 col-sm-12">
                <div class="copyright-right">
                    <ul class="social-copyright">
                        <% if (settings.socialLinks.facebook) { %>
                        <li><a href="<%= settings.socialLinks.facebook%>"><i data-feather="facebook"></i></a></li>
                        <% } if (settings.socialLinks.twitter) { %>
                        <li><a href="<%= settings.socialLinks.twitter%>"><i data-feather="twitter"></i></a></li>
                        <% } if (settings.socialLinks.instagram) { %>
                        <li><a href="<%= settings.socialLinks.instagram%>"><i data-feather="instagram"></i></a></li>
                        <% } if (settings.socialLinks.linkedin) { %>
                        <li><a href="<%= settings.socialLinks.linkedin%>"><i data-feather="linkedin"></i></a></li>
                        <% } if (settings.socialLinks.youtube) { %>
                        <li><a href="<%= settings.socialLinks.youtube%>"><i data-feather="youtube"></i></a></li>
                        <% } %>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Footer Area -->
<div class="mouse-cursor cursor-outer"></div>
<div class="mouse-cursor cursor-inner"></div>
<!-- Start Top To Bottom Area  -->
<div class="rn-progress-parent">
    <svg class="rn-back-circle svg-inner" width="100%" height="100%" viewBox="-1 -1 102 102">
        <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" />
    </svg>
</div>
<!-- End Top To Bottom Area  -->
